
{
  "description": "Wasserstein competitive trainer",
  "discriminator": 
  {
    "class": "class:hypergan.discriminators.configurable_discriminator.ConfigurableDiscriminator",
    "defaults":{
      "filter": [3,3],
      "stride": [1,1],
      "avg_pool": [2,2],
      "activation": "relu",
      "initializer": "he_normal",
      "lrmul": 1.0,
      "random_stddev": 0.02
    },
    "layers":[
      "conv 64 ", 
      "conv 128",
      "conv 256",
      "conv 512",
      "linear 1 activation=null initializer=he_normal bias=false"
    ]
  },
  "latent":
  {
    "class": "function:hypergan.distributions.uniform_distribution.UniformDistribution",
    "min": -1,
    "max": 1,
    "z": 100
  },
 
  "generator": {
    "class": "class:hypergan.discriminators.configurable_discriminator.ConfigurableDiscriminator",
    "defaults":{
      "filter": [3,3],
      "stride": [1,1],
      "avg_pool": [1,1],
      "activation": "relu",
      "initializer": "he_normal",
      "lrmul": 1.0,
      "random_stddev": 0.02
    },
    "layers":[

      "resize_conv 128",
      "resize_conv 64",
      "resize_conv 3 activation=tanh"

    ]
  },
  "encoder": {
    "class": "class:hypergan.discriminators.configurable_discriminator.ConfigurableDiscriminator",
    "defaults":{
      "filter": [3,3],
      "stride": [1,1],
      "avg_pool": [2,2],
      "activation": "relu",
      "initializer": "he_normal",
      "lrmul": 1.0,
      "random_stddev": 0.02
    },
    "layers":[
      "conv 64 ", 
      "conv 128",
      "conv 256"

    ]
  },


 "loss": {
  "class": "function:hypergan.losses.standard_loss.StandardLoss",
    "reduce": "reduce_mean"
  },
    "trainer": {
      "class": "function:hypergan.trainers.simultaneous_trainer.SimultaneousTrainer",
      "optimizer": {
          "class": "function:tensorflow.python.training.adam.AdamOptimizer",
          "learn_rate": 1e-4
      },
      "hooks":[
      {
        "class": "function:hypergan.train_hooks.stabilizing_training_train_hook.StabilizingTrainingTrainHook",
        "gamma": 1.0
      }

      ]
  },

    "runtime": {
    "anysize": true,
    "train": "hypergan train [dataset] --sampler static_batch -b 8 --size 64x64x3"
  },
  "hypergan_version": "0.10.0",
  "d1_lambda": 1.0,
  "shared_encoder": true,
  "class": "class:hypergan.gans.multi_marginal_gan.MultiMarginalGAN"
}
