{
  "name": "qviz",
  "version": "0.1.0",
  "description": "Qbeast's Jupyterlab widgets",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/Qbeast-io/qviz",
  "bugs": {
    "url": "https://github.com/Qbeast-io/qviz/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Cesare Cugnasco",
    "email": "cesare@qbeast.io"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Qbeast-io/qviz"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:nbextension",
    "build:labextension": "npm run clean:labextension && mkdirp qviz/labextension && cd qviz/labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:nbextension": "webpack -p",
    "build:all": "npm run build:labextension && npm run build:nbextension",
    "clean": "npm run clean:lib && npm run clean:nbextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf qviz/labextension",
    "clean:nbextension": "rimraf qviz/nbextension/static/index.js",
    "prepack": "npm run build:lib",
    "test": "npm run test:firefox",
    "test:chrome": "karma start --browsers=Chrome tests/karma.conf.js",
    "test:debug": "karma start --browsers=Chrome --singleRun=false --debug=true tests/karma.conf.js",
    "test:firefox": "karma start --browsers=Firefox tests/karma.conf.js",
    "test:ie": "karma start --browsers=IE tests/karma.conf.js",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack --watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^3.0.0",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@mdi/js": "^4.4.95",
    "@svgr/webpack": "^4.3.2",
    "@types/dat.gui": "^0.7.5",
    "acorn": "^7.1.1",
    "apache-arrow": "^0.17.0",
    "chroma-js": "^2.0.6",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "connected-react-router": "^6.5.2",
    "dat.gui": "^0.7.7",
    "history": "^4.10.1",
    "karma-script-launcher": "^1.0.0",
    "react": "^16.8.4",
    "react-autosuggest": "^9.4.3",
    "react-data-grid": "^6.1.0",
    "react-data-grid-addons": "^6.1.0",
    "react-dom": "^16.8.4",
    "react-grid-layout": "^0.16.6",
    "react-hot-keys": "^2.2.0",
    "react-idle-timer": "^4.2.9",
    "resize-observer-polyfill": "^1.5.1",
    "semver": "^7.3.2",
    "three": "^0.116.1",
    "tinycolor2": "^1.4.1",
    "uos": "^1.2.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.6",
    "@types/webpack-env": "^1.13.6",
    "bufferutil": "^4.0.1",
    "css-loader": "^3.2.0",
    "expect.js": "^0.3.1",
    "fs-extra": "^7.0.0",
    "karma": "^4.4.1",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^5.0.2",
    "karma-typescript-es6-transform": "^5.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^5.2.1",
    "typescript": "^3.8.3",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
