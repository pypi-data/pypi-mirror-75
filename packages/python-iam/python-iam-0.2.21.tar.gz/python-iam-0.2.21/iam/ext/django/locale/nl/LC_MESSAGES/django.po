#: jinja2/iam/login.html.j2:62
msgid "Login"
msgstr "Aanmelden"

#: jinja2/iam/login.html.j2:80
msgid "Choose one of the options below to sign in."
msgstr "Kies uit één van de onderstaande opties om je aan te melden."

#: jinja2/iam/login.html.j2:99 jinja2/iam/register.html.j2:53
msgid "Password"
msgstr "Wachtwoord"

#: jinja2/iam/login.html.j2:105
msgid "Sign in"
msgstr "Aanmelden"

#: jinja2/iam/login.html.j2:114
msgid "Forgot password?"
msgstr "Wachtwoord vergeten?"

#: jinja2/iam/login.html.j2:126
msgid "Send magic link"
msgstr "Verstuur een magic link"

#: jinja2/iam/login.html.j2:136
msgid "Register / login with Google"
msgstr "Registreren / aanmelden met Google"

#: jinja2/iam/login.html.j2:146
msgid "Register / login with Twitter"
msgstr "Registreren / aanmelden met Twitter"

#: jinja2/iam/login.html.j2:156
msgid "Register / login with Facebook"
msgstr "Registreren / aanmelden met Facebook"

#: jinja2/iam/login.html.j2:166
msgid "Register / login with LinkedIn"
msgstr "Registreren / aanmelden met LinkedIn"

#: jinja2/iam/login.html.j2:175
msgid "Register a new account"
msgstr "Registreer een nieuwe account"

#: jinja2/iam/login.magic.html.j2:8
msgid "A magic link allows you login with just one click."
msgstr "Met een magic link kun je inloggen met één klik."

#: jinja2/iam/login.magic.html.j2:9
msgid "Each magic link is unique and can only be used once."
msgstr "Een magic link is uniek en kan maar één keer gebruikt worden."

#: jinja2/iam/login.magic.html.j2:16
msgid "Send my magic link"
msgstr "Stuur mij een magic link"

